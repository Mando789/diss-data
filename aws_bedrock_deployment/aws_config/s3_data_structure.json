{
  "metadata": {
    "description": "S3 bucket organization and data structure for workflow optimization system",
    "version": "1.0",
    "purpose": "Data storage, training materials, and system artifacts",
    "security": "Private bucket with IAM role-based access"
  },
  "bucket_configuration": {
    "bucket_name": "workflow-optimization-data",
    "region": "us-east-1",
    "versioning": "enabled",
    "encryption": "AES256",
    "lifecycle_policies": {
      "training_data": "Retain indefinitely",
      "processing_temp": "Delete after 30 days",
      "logs": "Transition to IA after 30 days, delete after 1 year",
      "outputs": "Retain for 2 years"
    },
    "access_control": {
      "bucket_policy": "private",
      "iam_roles": ["lambda-execution-role", "bedrock-service-role"],
      "cross_origin": false
    }
  },
  "directory_structure": {
    "training_data/": {
      "description": "Training datasets for agent fine-tuning",
      "subdirectories": {
        "input_agent/": {
          "files": [
            "input_agent_training.jsonl",
            "input_validation_cases.json",
            "multimodal_examples/"
          ],
          "size_estimate": "50MB",
          "access_pattern": "Read-heavy during training"
        },
        "analysis_agent/": {
          "files": [
            "analysis_agent_training.jsonl",
            "framework_violation_examples.json",
            "benchmark_analyses.json"
          ],
          "size_estimate": "75MB",
          "access_pattern": "Read-heavy during training and inference"
        },
        "optimization_agent/": {
          "files": [
            "optimization_agent_training.jsonl",
            "roi_calculation_examples.json",
            "implementation_patterns.json"
          ],
          "size_estimate": "60MB",
          "access_pattern": "Read-heavy during training"
        },
        "output_agent/": {
          "files": [
            "output_agent_training.jsonl",
            "format_templates/",
            "presentation_examples.json"
          ],
          "size_estimate": "40MB",
          "access_pattern": "Read-heavy during training and inference"
        },
        "combined/": {
          "files": [
            "combined_training_dataset.jsonl",
            "multi_agent_examples.json",
            "integration_test_cases.json"
          ],
          "size_estimate": "100MB",
          "access_pattern": "Read-heavy during multi-agent training"
        }
      }
    },
    "framework_knowledge/": {
      "description": "Framework knowledge base for agent reference",
      "subdirectories": {
        "agile/": {
          "files": [
            "agile_framework_knowledge.json",
            "agile_principles_detailed.json",
            "agile_violation_patterns.json"
          ],
          "size_estimate": "15MB",
          "access_pattern": "Read-heavy during inference"
        },
        "lean/": {
          "files": [
            "lean_framework_knowledge.json",
            "seven_wastes_detection.json",
            "lean_optimization_patterns.json"
          ],
          "size_estimate": "20MB",
          "access_pattern": "Read-heavy during inference"
        },
        "operating_model/": {
          "files": [
            "operating_model_knowledge.json",
            "organizational_patterns.json",
            "governance_frameworks.json"
          ],
          "size_estimate": "18MB",
          "access_pattern": "Read-heavy during inference"
        },
        "integrated/": {
          "files": [
            "integrated_optimization_rules.json",
            "framework_synergies.json",
            "multi_framework_patterns.json"
          ],
          "size_estimate": "25MB",
          "access_pattern": "Read-heavy during analysis and optimization"
        },
        "prompts/": {
          "files": [
            "system_prompts.json",
            "agent_specific_prompts/",
            "prompt_templates.json"
          ],
          "size_estimate": "10MB",
          "access_pattern": "Read-heavy during agent initialization"
        }
      }
    },
    "validation_data/": {
      "description": "Test cases and validation datasets",
      "subdirectories": {
        "test_cases/": {
          "files": [
            "validation_cases.json",
            "benchmark_examples.json",
            "university_test_data.json"
          ],
          "size_estimate": "30MB",
          "access_pattern": "Read during testing and validation"
        },
        "performance_metrics/": {
          "files": [
            "accuracy_benchmarks.json",
            "quality_standards.json",
            "success_criteria.json"
          ],
          "size_estimate": "5MB",
          "access_pattern": "Read during performance evaluation"
        },
        "research_validation/": {
          "files": [
            "bpi_challenge_benchmarks.json",
            "academic_standards.json",
            "empirical_validation.json"
          ],
          "size_estimate": "15MB",
          "access_pattern": "Read during academic validation"
        }
      }
    },
    "bpi_challenge_data/": {
      "description": "Processed BPI Challenge 2020 datasets",
      "subdirectories": {
        "raw_data/": {
          "files": [
            "DomesticDeclarations.xes",
            "InternationalDeclarations.xes",
            "PermitLog.xes",
            "PrepaidTravelCost.xes",
            "RequestForPayment.xes"
          ],
          "size_estimate": "500MB",
          "access_pattern": "Read during data processing and analysis"
        },
        "processed_features/": {
          "files": [
            "domestic_declarations_features.csv",
            "international_declarations_features.csv",
            "travel_permits_features.csv",
            "combined_features.csv"
          ],
          "size_estimate": "100MB",
          "access_pattern": "Read during training and validation"
        },
        "labeled_data/": {
          "files": [
            "domestic_declarations_labeled.json",
            "international_declarations_labeled.json",
            "combined_labeled_dataset.json"
          ],
          "size_estimate": "150MB",
          "access_pattern": "Read during training and research validation"
        }
      }
    },
    "processing_temp/": {
      "description": "Temporary files during workflow processing",
      "subdirectories": {
        "input_processing/": {
          "files": ["Temporary input files", "Processing artifacts"],
          "retention": "24 hours",
          "access_pattern": "Write-heavy during processing"
        },
        "analysis_temp/": {
          "files": ["Intermediate analysis results", "Framework assessments"],
          "retention": "24 hours",
          "access_pattern": "Read-write during analysis"
        },
        "optimization_temp/": {
          "files": ["Recommendation drafts", "ROI calculations"],
          "retention": "24 hours", 
          "access_pattern": "Read-write during optimization"
        }
      }
    },
    "outputs/": {
      "description": "Generated optimization results and reports",
      "subdirectories": {
        "executive_summaries/": {
          "files": ["Executive summary reports"],
          "retention": "2 years",
          "access_pattern": "Write during processing, read for delivery"
        },
        "detailed_analyses/": {
          "files": ["Comprehensive analysis reports"],
          "retention": "2 years",
          "access_pattern": "Write during processing, read for delivery"
        },
        "implementation_guides/": {
          "files": ["Implementation roadmaps and guides"],
          "retention": "2 years",
          "access_pattern": "Write during processing, read for delivery"
        },
        "business_cases/": {
          "files": ["Business justification documents"],
          "retention": "2 years",
          "access_pattern": "Write during processing, read for delivery"
        }
      }
    },
    "system_logs/": {
      "description": "System operation and performance logs",
      "subdirectories": {
        "agent_performance/": {
          "files": ["Agent execution logs", "Performance metrics"],
          "retention": "90 days",
          "access_pattern": "Write-heavy, read for monitoring"
        },
        "quality_metrics/": {
          "files": ["Quality assessment logs", "Accuracy measurements"],
          "retention": "1 year",
          "access_pattern": "Write during processing, read for analysis"
        },
        "error_logs/": {
          "files": ["Error reports", "Debug information"],
          "retention": "6 months",
          "access_pattern": "Write during errors, read for troubleshooting"
        }
      }
    },
    "documentation/": {
      "description": "System documentation and data dictionaries",
      "subdirectories": {
        "data_dictionary/": {
          "files": [
            "data_dictionary.json",
            "schema_definitions.json",
            "field_descriptions.json"
          ],
          "retention": "Permanent",
          "access_pattern": "Read for reference"
        },
        "api_documentation/": {
          "files": [
            "api_specifications.json",
            "endpoint_documentation.json",
            "usage_examples.json"
          ],
          "retention": "Permanent",
          "access_pattern": "Read for development and integration"
        },
        "deployment_guides/": {
          "files": [
            "deployment_instructions.md",
            "configuration_guide.md",
            "troubleshooting_guide.md"
          ],
          "retention": "Permanent",
          "access_pattern": "Read during deployment and maintenance"
        }
      }
    }
  },
  "access_patterns": {
    "high_frequency_read": [
      "framework_knowledge/",
      "training_data/",
      "prompts/"
    ],
    "moderate_frequency_read": [
      "validation_data/",
      "bpi_challenge_data/processed_features/"
    ],
    "low_frequency_read": [
      "bpi_challenge_data/raw_data/",
      "documentation/"
    ],
    "high_frequency_write": [
      "processing_temp/",
      "system_logs/"
    ],
    "moderate_frequency_write": [
      "outputs/"
    ]
  },
  "performance_optimization": {
    "caching_strategy": {
      "framework_knowledge": "Cache in Lambda memory for 15 minutes",
      "training_data": "Cache during training sessions",
      "prompts": "Cache in Lambda memory permanently"
    },
    "compression": {
      "json_files": "gzip compression for files >1MB",
      "logs": "gzip compression for all log files",
      "outputs": "zip compression for delivery"
    },
    "transfer_acceleration": "Enabled for large file uploads",
    "multipart_uploads": "Enabled for files >100MB"
  },
  "security_configuration": {
    "encryption": {
      "at_rest": "AES256",
      "in_transit": "TLS 1.2+",
      "key_management": "AWS KMS"
    },
    "access_control": {
      "bucket_policy": "Deny all public access",
      "iam_policies": "Principle of least privilege",
      "vpc_endpoints": "Enabled for private access"
    },
    "monitoring": {
      "access_logging": "Enabled",
      "cloudtrail": "Enabled for all API calls",
      "vpc_flow_logs": "Enabled"
    }
  }
}